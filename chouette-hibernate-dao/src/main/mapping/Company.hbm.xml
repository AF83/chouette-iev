<hibernate-mapping package="fr.certu.chouette.model.neptune">
  <class name="Company" table="Company" dynamic-insert="false" dynamic-update="false" >
    <id name="id" type="long">
      <generator class="native"/>
    </id>
    <property name="objectId" type="string" unique="true" not-null="true"/>
    <property name="objectVersion" type="int"/>
    <property name="creationTime" type="java.util.Date"/>
    <property name="creatorId" type="string"/>
    <property name="name" not-null="true"/>
    <property name="shortName"/>
    <property name="organisationalUnit" column="organizationalUnit"/>
    <property name="operatingDepartmentName"/>
    <property name="code"/>
    <property name="phone"/>
    <property name="fax"/>
    <property name="email"/>
    <property name="registrationNumber" unique="true"/>
  </class>
</hibernate-mapping>

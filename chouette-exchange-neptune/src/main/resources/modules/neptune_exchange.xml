<?xml version="1.0" encoding="UTF-8"?>
<beans 	
	xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean id="NeptuneLineImport" class="fr.certu.chouette.exchange.xml.neptune.XMLNeptuneImportLinePlugin">
  </bean>

  <bean id="NeptuneLinePluginInjector" class="fr.certu.chouette.plugin.PluginInjector" init-method="init" factory-method="createLinePluginInjector" >
		<property name="manager" ref="lineManager"/>
		<property name="importPlugins">
			<list>
				<ref bean="NeptuneLineImport" />
			</list>
		</property>
		<property name="exportPlugins">
			<list>
			</list>
		</property>
		<property name="exportDeletionPlugins">
			<list>
			</list>
		</property>
  
  </bean>

</beans>

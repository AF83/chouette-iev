<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="fr.certu.chouette.modele.Reseau"
        table="PtNetwork"
        dynamic-update="false"
        dynamic-insert="false">

		<id name="id" column="id" type="long">
			<generator class="native"/>
		</id>

        <property name="objectId" type="string" unique="true" column="objectId" />
		<property name="objectVersion" type="int" column="objectVersion" />
		<property name="creationTime" type="java.util.Date" column="creationTime" />
		<property name="creatorId" type="string" column="creatorId" />

	    <property name="versionDate" type="date" column="versionDate" />
	    <property name="description" column="description" />
	    <property name="name" column="name" />
		<property name="registrationNumber" column="registrationNumber" unique="true"/>
	    <property name="sourceName" column="sourceName" />
	    <property name="sourceIdentifier" column="sourceIdentifier" />
	    <property name="comment" column="comment" />
    </class>
</hibernate-mapping>
